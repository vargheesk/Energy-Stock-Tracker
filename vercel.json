{
  "version": 2,
  "builds": [
    {
      "src": "etl.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/etl",
      "dest": "etl.py"
    }
  ],
  "env": {
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_KEY": "@supabase_key"
  },
  "functions": {
    "etl.py": {
      "maxDuration": 300
    }
  }
}